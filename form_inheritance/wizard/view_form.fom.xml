<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_form_wizard" model="ir.ui.view">
        <field name="name">wizard</field>
        <field name="model">form.wizard</field>
        <field name="arch" type="xml">
          <form>
            <group>
               <field name="current_date"/>
               <field name="current_date"/>
            </group>
            <footer>
              <button type="object"
                name="button_send"
                string="Ver calendario"
                />
              <button special="cancel"
                string="Cancel"
                class="btn-secondary" />
            </footer>
          </form>
        </field>
      </record>
    <act_window  
    id="action_form"
    name="Inscripciones"
    res_model = "form.form"  
    view_mode="wizard,tree,form" /> 
    
    <menuitem id="menu_form_form_wizard"
        name="Inscripciones" 
        parent ="form_app.menu_form" 
        action="action_form"
        sequence="50"/> 
</odoo>